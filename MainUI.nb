Cell[BoxData[
 RowBox[{"{", 
  RowBox[{
  "\"\<Consider the curve defined by the polar equation r = \[Alpha] , 0 <= \
\[Theta] <= 2\[Pi].\\nA. Show that the point [\[Mu],\[Gamma]] lies on the \
curve and find the Cartesian coordinates of this point\\nB. Find the polar \
coordinates of (\[Omega],\[Delta])\\nC. Sketch a graph of the curve.\\nD. \
Find a cartesian equation of the curve. (The answer shown won't be reduced \
most of the time.)\>\"", ",", 
   RowBox[{"{", 
    RowBox[{
    "\[Alpha]", ",", "\[Mu]", ",", "\[Gamma]", ",", "\[Omega]", ",", 
     "\[Delta]"}], "}"}], ",", 
   RowBox[{"Hold", "[", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "func", ",", "angle", ",", "dist", ",", "recX", ",", "recY", ",", 
        "recAngle"}], "}"}], ",", 
      RowBox[{
       RowBox[{"func", "=", 
        RowBox[{"RandomPolar", "[", "]"}]}], ";", 
       RowBox[{"angle", "=", 
        RowBox[{"RandomChoice", "[", "angleSet", "]"}]}], ";", 
       RowBox[{"dist", "=", 
        RowBox[{"func", "/.", "\[VeryThinSpace]", 
         RowBox[{"\[Theta]", "\[Rule]", "angle"}]}]}], ";", 
       RowBox[{"recAngle", "=", 
        RowBox[{"RandomChoice", "[", "angleSet", "]"}]}], ";", 
       RowBox[{"recX", "=", 
        RowBox[{"Simplify", "[", 
         RowBox[{
          RowBox[{"Cos", "[", "recAngle", "]"}], " ", 
          RowBox[{"(", 
           RowBox[{"func", "/.", "\[VeryThinSpace]", 
            RowBox[{"\[Theta]", "\[Rule]", "recAngle"}]}], ")"}]}], "]"}]}], 
       ";", 
       RowBox[{"recY", "=", 
        RowBox[{"Simplify", "[", 
         RowBox[{
          RowBox[{"Sin", "[", "recAngle", "]"}], " ", 
          RowBox[{"(", 
           RowBox[{"func", "/.", "\[VeryThinSpace]", 
            RowBox[{"\[Theta]", "\[Rule]", "recAngle"}]}], ")"}]}], "]"}]}], 
       ";", 
       RowBox[{"{", 
        RowBox[{"func", ",", "dist", ",", "angle", ",", "recX", ",", "recY"}],
         "}"}]}]}], "]"}], "]"}], ",", 
   RowBox[{"Hold", "[", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "coords", ",", "r", ",", "angle", ",", "plot", ",", "eqn", ",", 
        "simp"}], "}"}], ",", 
      RowBox[{
       RowBox[{"coords", "=", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"\[Mu]", " ", 
           RowBox[{"Cos", "[", "\[Gamma]", "]"}]}], ",", 
          RowBox[{"\[Mu]", " ", 
           RowBox[{"Sin", "[", "\[Gamma]", "]"}]}]}], "}"}]}], ";", 
       RowBox[{"r", "=", 
        RowBox[{"RootReduce", "[", 
         RowBox[{"Simplify", "[", 
          RowBox[{"Abs", "[", 
           RowBox[{"\[Omega]", "+", 
            RowBox[{"\[Delta]", " ", "\[ImaginaryI]"}]}], "]"}], "]"}], 
         "]"}]}], ";", 
       RowBox[{"angle", "=", 
        RowBox[{"Simplify", "[", 
         RowBox[{
          RowBox[{"Solve", "[", 
           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"Tan", "[", "x", "]"}], "\[Equal]", 
              FractionBox["\[Delta]", "\[Omega]"]}], "&&", 
             RowBox[{"0", "\[LessEqual]", "x", "\[LessEqual]", 
              RowBox[{"2", " ", "\[Pi]"}]}]}], ",", "x"}], "]"}], 
          "\[LeftDoubleBracket]", 
          RowBox[{"1", ",", "1", ",", "2"}], "\[RightDoubleBracket]"}], 
